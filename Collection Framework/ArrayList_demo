import java.util.ArrayList;

public class ArrayList_demo {

	public static void main(String[] args) {

		ArrayList l = new ArrayList();
		l.add("A");
		l.add(10);
		l.add("z");
		l.add(null);
		System.out.println(l);
		l.remove(2);
		System.out.println(l);
		l.add(2, "z");
		System.out.println(l);
		
	}

}
